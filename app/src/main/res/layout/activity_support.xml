<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
      android:background="@drawable/bg_add_factory"
     tools:context="com.example.shaymaa.finalproject.activites.SupportActivity">
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/colorPrimaryDark"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp" >
        <com.example.shaymaa.finalproject.others.MyTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#fff"
            android:text="الدعم الفني"
            android:id="@+id/wasf_titel"
            />
        <ImageView
            android:id="@+id/go_back"
            android:layout_gravity="end"
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:padding="@dimen/_4sdp"
            android:layout_margin="@dimen/_10sdp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:src="@drawable/go_back"
            android:contentDescription="@string/bake_icon" />
    </android.support.v7.widget.Toolbar>




        <LinearLayout
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <ImageView
                android:layout_marginTop="@dimen/_13sdp"
                android:layout_marginBottom="@dimen/_5sdp"
                android:layout_gravity="center"
                android:layout_width="@dimen/_50sdp"
                android:padding="@dimen/_10sdp"
                android:layout_height="@dimen/_50sdp"
                android:id="@+id/imageView"
                android:background="@drawable/suuportt"
                />


            <TextView
                android:layout_marginRight="@dimen/_35sdp"
                android:layout_gravity="center"
                android:textColor="#ffffff"
                android:textSize="14sp"
                android:layout_marginLeft="@dimen/_30sdp"
                android:padding="@dimen/_2sdp"
                android:textStyle="bold"
                android:text="الدعم الفني"
                android:textAlignment="center"
                 android:layoutDirection="rtl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                 />

            <EditText
                 android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_gravity="center"
                android:layout_width="@dimen/_240sdp"
                android:padding="@dimen/_5sdp"
                android:background="@drawable/round_white"
                android:textColorHint="#fff"
                android:textColor="#fff"
                android:layout_height="@dimen/_40sdp"
                android:drawablePadding="@dimen/_15sdp"
                android:inputType="textPersonName"
                android:hint="الأسم"
                android:ems="10"
                android:drawableRight="@drawable/ic_perm_identity_white_24dp"
                android:id="@+id/editText_nam"

                />

            <EditText
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_gravity="center"
                android:layout_width="@dimen/_240sdp"
                android:layout_height="@dimen/_40sdp"
                android:paddingTop="@dimen/_8sdp"
                android:padding="@dimen/_5sdp"
                android:background="@drawable/round_white"
                android:textColorHint="#fff"
                android:textColor="#fff"
                 android:drawableRight="@drawable/ic_phone_android_white"
                android:hint="رقم الجوال"
                android:drawablePadding="@dimen/_15sdp"
                android:inputType="textPhonetic"
                android:ems="10"
                android:id="@+id/editText_phone" />

            <EditText
                android:hint="البريد الألكتروني "
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_gravity="center"
                android:layout_width="@dimen/_240sdp"
                android:layout_height="@dimen/_40sdp"
                android:padding="@dimen/_5sdp"
                android:paddingRight="@dimen/_15sdp"
                android:background="@drawable/round_white"
                android:textColorHint="#fff"
                android:textColor="#fff"
                android:drawablePadding="@dimen/_15sdp"
                 android:inputType="textEmailAddress"
                android:drawableRight="@drawable/ic_mail_outline_blwhitk_24dp"
                android:ems="10"
                android:id="@+id/editText_email" />
            <EditText
                android:hint= "عنوان  الرساله"
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_gravity="center"
                android:layout_width="@dimen/_240sdp"
                android:layout_height="@dimen/_40sdp"
                android:padding="@dimen/_5sdp"
                android:paddingRight="@dimen/_15sdp"
                android:background="@drawable/round_white"
                android:textColorHint="#fff"
                android:textColor="#fff"
                android:drawablePadding="@dimen/_15sdp"
                 android:inputType="text"
                android:drawableRight="@drawable/ic_mail_outline_blwhitk_24dp"
                android:ems="10"
                android:id="@+id/editText_address" />


            <EditText
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_gravity="center"
                android:layout_width="@dimen/_240sdp"
                android:paddingStart="@dimen/_5sdp"
                android:layout_height="@dimen/_80sdp"
                android:gravity="right"
                android:hint="نص الرساله "
                android:paddingTop="@dimen/_10sdp"
                android:textColorHint="#fff"
                android:textColor="#fff"
                android:drawablePadding="@dimen/_15sdp"
                android:drawableRight="@drawable/ic_mail_outline_blwhitk_24dp"
                android:paddingBottom="@dimen/_8sdp"
                android:background="@drawable/round_white"
                android:inputType="text"            android:ems="10"
                android:id="@+id/contain_massge" />



            <Button
                android:textColor="#ffffff"
                android:background="@drawable/login_btn"
                android:layout_gravity="center"
                android:text="أكمل التسجيل "
                android:textSize="20sp"
                android:layout_width="@dimen/_220sdp"
                android:layout_height="@dimen/_35sdp"
                android:id="@+id/sed_button"/>


        </LinearLayout>



</LinearLayout>
