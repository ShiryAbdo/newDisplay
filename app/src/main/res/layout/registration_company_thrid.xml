<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/smoke_chimney_industrial_29465"
    tools:context="com.example.shaymaa.finalproject.activites.RegistrationUser">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:orientation="vertical">

        <ImageView


            android:id="@+id/imageView"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"

            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/_5sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:background="@drawable/storage_copy"
            android:padding="@dimen/_5sdp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="تسجيل  الشركات والمؤسسات "
            android:textColor="#01048e"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/checkBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/_5sdp"
            android:layoutDirection="rtl"
             android:text="___________________________________________________ "
            android:textColor="#ffffff"
            android:textSize="10sp"
            android:textStyle="bold" />

        <EditText
            android:paddingRight="@dimen/_10sdp"
            android:id="@+id/number_of_fax"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/_8sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/fild_redise_blue"
            android:drawablePadding="@dimen/_15sdp"
            android:drawableRight="@drawable/ic_person_outline_white_24dp"
            android:ems="10"
            android:hint="رقم الفاكس"
            android:inputType="number"


            />

        <EditText
            android:id="@+id/web_url"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/_8sdp"
            android:background="@drawable/fild_redise_blue"
            android:drawablePadding="@dimen/_15sdp"
            android:drawableRight="@drawable/ic_mail_outline_black_24dp"
            android:ems="10"
            android:hint="الموقغ الإلكتروني"
            android:inputType="textUri"
            android:paddingBottom="@dimen/_8sdp"
            android:paddingRight="@dimen/_10sdp"
            android:paddingTop="@dimen/_8sdp" />

        <EditText
            android:id="@+id/name_of_head"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/_10sdp"
            android:background="@drawable/fild_redise_blue"
            android:drawablePadding="@dimen/_15sdp"
            android:drawableRight="@drawable/ic_phone_android_black_24dp"
            android:ems="10"
            android:hint="اسم المدير العام"
            android:inputType="text"
            android:paddingRight="@dimen/_10sdp" />

        <EditText
            android:drawablePadding="@dimen/_15sdp"
            android:id="@+id/represint_company"
            android:hint="ممثل الشركه"
            android:layout_width="@dimen/_240sdp"
            android:drawableRight="@drawable/ic_phone_android_black_24dp"
            android:layout_height="@dimen/_37sdp"
            android:layout_gravity="center"
             android:layout_marginBottom="@dimen/_8sdp"
             android:background="@drawable/fild_redise_blue"
            android:padding="@dimen/_10sdp"
            android:paddingBottom="@dimen/_8sdp"
            android:paddingRight="@dimen/_10sdp"
            android:paddingTop="@dimen/_8sdp" />

        <EditText
            android:id="@+id/numer_segil_togary"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="center"
             android:layout_marginBottom="@dimen/_8sdp"
             android:background="@drawable/fild_redise_blue"
            android:drawablePadding="@dimen/_15sdp"
            android:drawableRight="@drawable/ic_lock_outline_white_24dp"
            android:ems="10"
            android:gravity="right"
            android:hint="رقم السجل التجاري"
            android:inputType="number"
            android:paddingBottom="@dimen/_7sdp"
            android:paddingRight="@dimen/_10sdp"
            android:paddingTop="@dimen/_10sdp" />

        <EditText
            android:paddingTop="@dimen/_10sdp"
            android:paddingStart="@dimen/_8sdp"
            android:id="@+id/password"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/_8sdp"
            android:background="@drawable/round_white"
            android:drawablePadding="@dimen/_15sdp"
            android:drawableRight="@drawable/ic_lock_outline_white_24dp"
            android:ems="10"
            android:gravity="right"
            android:hint="كلمة المرور"
            android:inputType="textPassword"
            android:textColor="#fff"
            android:textColorHint="#fff" />

        <EditText
            android:paddingStart="@dimen/_8sdp"
            android:id="@+id/re_password"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/_5sdp"
            android:background="@drawable/round_white"
            android:drawablePadding="@dimen/_15sdp"
            android:drawableRight="@drawable/ic_lock_outline_white_24dp"
            android:ems="10"
            android:gravity="right"
            android:hint="   إعادة كلمة المرور  "
             android:inputType="textPassword"
            android:paddingBottom="@dimen/_8sdp"
            android:paddingRight="@dimen/_10sdp"
            android:paddingTop="@dimen/_8sdp"
            android:textColor="#fff"
            android:textColorHint="#fff" />


        <Button
            android:id="@+id/complet_register_moassa"
            android:layout_width="@dimen/_220sdp"
            android:layout_height="@dimen/_37sdp"
            android:layout_gravity="center"
            android:background="@drawable/login_btn"
            android:paddingRight="@dimen/_10sdp"
            android:text= "تسجيل"
            android:textColor="#ffffff"
            android:textSize="20sp" />


    </LinearLayout>
</FrameLayout>

