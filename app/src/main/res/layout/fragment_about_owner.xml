<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/about_owner"
    android:background="#d9f5f5f5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shaymaa.finalproject.fragments.AboutOFactory">

     <LinearLayout
         android:layout_width="match_parent"
         android:orientation="horizontal"
         android:layout_height="match_parent">
         <LinearLayout
             android:layout_width="0dp"
             android:orientation="vertical"
             android:layout_height="match_parent"
             android:layout_weight="1">
             <com.example.shaymaa.finalproject.MyTextView
                 android:textColor="@color/colorAccent"
                  android:textSize="12sp"
                 android:text=" اسم صاحب المصنع  :"
                 android:layout_weight=".5"
                 android:padding="@dimen/_5sdp"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="@dimen/_150sdp"
                 android:layout_height="wrap_content" />

             <com.example.shaymaa.finalproject.MyTextView
                 android:textColor="@color/colorAccent"
                  android:textSize="12sp"
                 android:padding="@dimen/_5sdp"
                 android:text="رقم الجوال               :"
                  android:layout_weight=".5"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="@dimen/_150sdp"
                 android:layout_height="wrap_content" />
             <com.example.shaymaa.finalproject.MyTextView
                 android:textColor="@color/colorAccent"
                  android:textSize="12sp"
                 android:padding="@dimen/_5sdp"
                 android:text="  البريد الإلكتروني    : "
                 android:layout_weight=".5"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content" />
             <com.example.shaymaa.finalproject.MyTextView
                  android:layout_weight="3"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content" />
             <com.example.shaymaa.finalproject.MyTextView
                  android:layout_weight="3"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="@dimen/_150sdp"
                 android:layout_height="wrap_content" />
             <com.example.shaymaa.finalproject.MyTextView
                  android:layout_weight="3"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="@dimen/_150sdp"
                 android:layout_height="wrap_content" />


         </LinearLayout>
         <LinearLayout
             android:layout_weight="1"
             android:layout_width="0dp"
             android:orientation="vertical"
             android:layout_height="match_parent">

             <com.example.shaymaa.finalproject.MyTextView
                 android:padding="@dimen/_5sdp"
                 android:textSize="12sp"
                 android:hint="اسم المصنع "
                  android:layout_weight=".5"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="@dimen/_150sdp"
                 android:layout_height="wrap_content" />

             <com.example.shaymaa.finalproject.MyTextView
                 android:padding="@dimen/_5sdp"
                 android:textSize="12sp"
                 android:hint="12345678 "
                 android:layout_weight=".5"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="@dimen/_150sdp"
                 android:layout_height="wrap_content" />
             <com.example.shaymaa.finalproject.MyTextView
                 android:padding="@dimen/_5sdp"
                 android:textSize="12sp"
                 android:hint="@string/email_hint"
                 android:layout_weight=".5"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content" />
             <com.example.shaymaa.finalproject.MyTextView
                 android:layout_weight="3"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content" />
             <com.example.shaymaa.finalproject.MyTextView
                 android:layout_weight="3"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="@dimen/_150sdp"
                 android:layout_height="wrap_content" />
             <com.example.shaymaa.finalproject.MyTextView
                 android:layout_weight="3"
                 android:gravity="right"
                 android:layout_gravity="center"
                 android:layout_width="@dimen/_150sdp"
                 android:layout_height="wrap_content" />

         </LinearLayout>
     </LinearLayout>

</FrameLayout>
