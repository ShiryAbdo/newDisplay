<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/about_owner"
    android:background="#d9f5f5f5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shaymaa.finalproject.fragments.AboutOFactory">

    <LinearLayout
        android:layout_margin="@dimen/_2sdp"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="horizontal"
            android:weightSum="3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:text= "   الحوال : "
                android:layout_weight="1"


                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />

            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:hint="  قسم اللإعلان  :"
                android:id="@+id/companymobile"
                android:layout_weight="2"
                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:weightSum="3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:text= "  الهاتف : "
                android:layout_weight="1"


                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />

            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:hint="--"
                android:id="@+id/companytel"
                android:layout_weight="2"
                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:weightSum="3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:text= " الفاكس : "
                android:layout_weight="1"


                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />

            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:hint="    تصنيف اللإعلان  :"
                android:id="@+id/companyfax"
                android:layout_weight="2"
                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:weightSum="3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:text= " الإيميل : "
                android:layout_weight="1"


                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />

            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:hint="السعر  :"
                android:id="@+id/companyemail"
                android:layout_weight="2"
                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:weightSum="3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:text= " المدينة : "
                android:layout_weight="1"


                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />

            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:hint=" -----  "
                android:id="@+id/cityname"
                android:layout_weight="2"
                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />


        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:weightSum="3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:text= "العنوان : "
                android:layout_weight="1"


                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />

            <com.example.shaymaa.finalproject.others.MyTextView
                android:textColor="@color/colorAccent"
                android:textSize="12sp"
                android:hint=" -----  "
                android:id="@+id/companyaddress"
                android:layout_weight="2"
                android:padding="@dimen/_2sdp"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />


        </LinearLayout>






    </LinearLayout>

</FrameLayout>
